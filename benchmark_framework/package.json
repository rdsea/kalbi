{
  "name": "benchmark_framework",
  "version": "0.1.0",
  "description": "",
  "main": "dist/main/Main.js",
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc --build tsconfig.json",
    "start": "node dist/main/Main.js $*",
    "start-debug": "node $NODE_DEBUG_OPTION dist/main/Main.js $*",
    "clean": "rm -rf dist",
    "build-start": "npm run build && npm run start",
    "build-start-debug": "npm run build && npm run start-debug",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha \"dist/test/**/*.js\"",
    "test-debug": "NODE_ENV=test ./node_modules/mocha/bin/mocha $NODE_DEBUG_OPTION \"dist/test/**/*.js\"",
    "build-test": "npm run build && npm run test",
    "build-test-debug": "npm run build && npm run test-debug"
  },
  "author": "Filip Rydzi",
  "devDependencies": {
    "@types/js-yaml": "^3.12.0",
    "typescript": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6"
  },
  "dependencies": {
    "@google-cloud/compute": "^0.12.0",
    "@types/node": "^10.11.4",
    "chai": "^4.2.0",
    "file-system": "^2.2.2",
    "fs-extra": "^7.0.1",
    "js-yaml": "^3.13.0",
    "log4js": "^3.0.6",
    "mocha": "^6.0.2",
    "path": "^0.12.7",
    "replace-in-file": "^3.4.2"
  }
}
